{
  "name": "plasticine-hooks",
  "version": "0.0.0",
  "author": "Plasticine-Yang",
  "description": "foo of @node-monorepo",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./useMount": {
      "require": "./dist/useMount/index.cjs",
      "import": "./dist/useMount/index.js",
      "types": "./dist/useMount/index.d.ts"
    },
    "./useUnmount": {
      "require": "./dist/useUnmount/index.cjs",
      "import": "./dist/useUnmount/index.js",
      "types": "./dist/useUnmount/index.d.ts"
    },
    "./useUnmountedRef": {
      "require": "./dist/useUnmountedRef/index.cjs",
      "import": "./dist/useUnmountedRef/index.js",
      "types": "./dist/useUnmountedRef/index.d.ts"
    },
    "./useSetState": {
      "require": "./dist/useSetState/index.cjs",
      "import": "./dist/useSetState/index.js",
      "types": "./dist/useSetState/index.d.ts"
    },
    "./useToggle": {
      "require": "./dist/useToggle/index.cjs",
      "import": "./dist/useToggle/index.js",
      "types": "./dist/useToggle/index.d.ts"
    },
    "./useBoolean": {
      "require": "./dist/useBoolean/index.cjs",
      "import": "./dist/useBoolean/index.js",
      "types": "./dist/useBoolean/index.d.ts"
    },
    "./useDebouncedFn": {
      "require": "./dist/useDebouncedFn/index.cjs",
      "import": "./dist/useDebouncedFn/index.js",
      "types": "./dist/useDebouncedFn/index.d.ts"
    }
  },
  "keywords": [
    "plasticine-hooks",
    "react hooks"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --dts --minify --sourcemap"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.195",
    "@types/react": "^18.2.10",
    "@types/react-dom": "^18.2.4",
    "fast-glob": "^3.2.12"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
